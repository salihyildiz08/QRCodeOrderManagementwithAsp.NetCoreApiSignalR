@model SignalRWebUI.Dtos.CategoryDtos.CreateBookingDto
@{
    ViewData["Title"] = "CreateCategory";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}



<div class="content">
    <div class="container-fluid">
        <h4 class="page-title">Add New Category</h4>
        <div class="row">
            <div class="col-md-12">
                <form method="post">
                    <div class="card">
                        <div class="card-header">
                            <div class="card-title">Add New Category</div>
                        </div>
                        <div class="card-body">
                            <div class="form-group">
                                <label for="categoryname">Category Name</label>
                                <input type="text" class="form-control" asp-for="CategoryName" placeholder="Category Name">
                            </div>

                        </div>
                        <div class="card-action">
                            <button class="btn btn-success">Add To Category</button>
                        </div>
                    </div>
                </form>
            </div>

        </div>
    </div>
</div>

