@model CreateSocialMediaDto
@{
    ViewData["Title"] = "CreateSocialMedia";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}



<div class="content">
    <div class="container-fluid">
        <h4 class="page-title">Add New SocialMedia</h4>
        <div class="row">
            <div class="col-md-12">
                <form method="post">
                    <div class="card">
                        <div class="card-header">
                            <div class="card-title">Add New SocialMedia</div>
                        </div>
                        <div class="card-body">
                            <div class="form-group">
                                <label for="Title">Title</label>
                                <input type="text" class="form-control" asp-for="Title" placeholder="Title">
                            </div>

                            <div class="form-group">
                                <label for="Url">Url</label>
                                <input type="text" class="form-control" asp-for="Url" placeholder="Url">
                            </div>

                            <div class="form-group">
                                <label for="Icon">Icon</label>
                                <input type="text" class="form-control" asp-for="Icon" placeholder="Icon">
                            </div>

                          
                        </div>
                        <div class="card-action">
                            <button class="btn btn-success">Add SocialMedia</button>
                        </div>
                    </div>
                </form>
            </div>

        </div>
    </div>
</div>
